Algoritmo notasdeclase
	definir nota1, nota2, nota3, notafinal Como Real;
	escribir "ingrese nota1";
	leer nota1;
	escribir "ingrese nota2";
	leer nota2;
	escribir "ingrese nota3";
	leer nota3;
	notafinal<- (nota1+nota2+nota3)/3;
	si notafinal<3.5
		entonces escribir " reprobaste el curso con una nota final de: " notafinal;
	SiNo
	 escribir "aprobasteel curso con nota final de: " notafinal;
	FinSi

	
FinAlgoritmo
